{
  "Create new rule for rhinestahl": {
    "description": "Create a new rule",
    "prefix": "cnrr",
    "body": [
      "{",
      "       \"clientId\": \"rhinestahl\", ",
      "       \"ruleId\": \"$1\",",
      "       \"body\": [$2]",
      "},"
    ]
  },
  "Create getItem dynamo handler": {
    "prefix": "cgidh",
    "body": [
      "{",
      "       \"handlerName\": \"dynamo\",",
      "       \"id\": \"$1\",",
      "       \"variables\": [",
      "{",
      "              \"action\": \"getItem\",",
      "              \"input\": {",
      "                     \"Key\": {",
      "                            \"id\": \"{$2}\",",
      "                            \"workItem\": \"{$3}\"",
      "},",
      "                     \"TableName\": \"{WORKFLOW_EXECUTION_TABLE}\"",
      "},",
      "                     \"targetPath\": \"$4\"",
      "}",
      "]",
      "},"
    ]
  },
  "Create deleteItem dynamo handler": {
    "prefix": "cddih",
    "body": [
      "{",
      "  \"handlerName\": \"dynamo\",",
      "  \"id\": \"$1\",",
      "  \"variables\": [",
      "     {",
      "       \"action\": \"deleteItem\",",
      "       \"input\": {",
      "              \"TableName\": \"{WORKFLOW_EXECUTION_TABLE}\",",
      "              \"Key\": {",
      "                     \"id\": \"{$2}\",",
      "                     \"workItem\": \"{$3}\",",
      "               }",
      "         },",
      "       \"targetPath\": \"$4\"",
      "     }",
      "   ],",
      "},"
    ]
  },
  "Create query dynamo handler": {
    "prefix": "cqdh",
    "body": [
      "{",
      "  \"handlerName\": \"dynamo\",",
      "  \"id\": \"$1\",",
      "  \"variables\": [",
      "     {",
      "      \"action\": \"query\",",
      "      \"input\": {",
      "        \"ExpressionAttributeNames\": {",
      "          \"#$2\": \"$3\",",
      "         },",
      "        \"ExpressionAttributeValues\": {",
      "          \":$4\": \"{$5}\",",
      "         },",
      "        \"FilterExpression\": \"$6\",",
      "        \"IndexName\": \"$7\",",
      "        \"KeyConditionExpression\": \"$8\",",
      "        \"TableName\": \"{WORKFLOW_EXECUTION_TABLE}\"",
      "      },",
      "      \"targetPath\": \"$9\"",
      "     }",
      "   ]",
      "},"
    ]
  },
  "Create updateItem dynamo handler": {
    "prefix": "cuidh",
    "body": [
      "{",
      "  \"handlerName\": \"dynamo\",",
      "  \"id\": \"$1\",",
      "  \"variables\": [",
      "    {",
      "    \"action\": \"updateItem\",",
      "    \"input\": {",
      "      \"TableName\": \"{WORKFLOW_EXECUTION_TABLE}\",",
      "      \"Key\": {",
      "        \"$2\": \"{$3}\",$4",
      "       },",
      "      \"ExpressionAttributeValues\": {",
      "          \":$5\": \"{$6}\"$7",
      "        },",
      "      \"UpdateExpression\": \"set $8\",",
      "     },",
      "    \"targetPath\": \"$9\"",
      "   }",
      " ]",
      "},"
    ]
  }
}
